{
  "name": "riku-on-device",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "build": {
    "appId": "ftlabs.riku",
    "files": [
      "./electron/build/**/*",
      "./electron/main.js"
    ],
    "extends": null,
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "dotenv": "^8.2.0",
    "grommet": "^2.10.0",
    "grommet-icons": "^4.4.0",
    "mqtt-react": "^0.0.3",
    "object.omit": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "styled-components": "^4.4.1",
    "voca": "^1.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf electron/build && react-scripts build && mv build electron/build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0"
  }
}
